[ignore]
.*/node_modules/fbjs/.*
<PROJECT_ROOT>/api/graphql/.*
<PROJECT_ROOT>/api/node_modules/.*
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/coverage/.*
<PROJECT_ROOT>/node_modules/auth0-lock
<PROJECT_ROOT>/node_modules/batchflow
<PROJECT_ROOT>/node_modules/editions/.*
<PROJECT_ROOT>/node_modules/reqwest

[options]
suppress_comment= \\(.\\|\n\\)*\\flow-ignore-next-line
module.name_mapper.extension='.*\.css$' -> 'CSSModule'
module.name_mapper.extension='.*\.png$' -> 'WebpackAsset'
module.name_mapper.extension='.*\.jpg$' -> 'WebpackAsset'
module.name_mapper.extension='.*\.svg$' -> 'WebpackAsset'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

[libs]
node_modules/iflow-lodash/index.js.flow

[version]
0.33.0
