{
  "extends": [
    "prettier",
    "prettier/react",
    "airbnb",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "prettier",
    "jest"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": "off",
    "react/no-array-index-key": "off",
    "function-paren-newline": "off",
    "comma-dangle": "off",
    "arrow-parens": "off"
  }
}
