{
  "name": "admin",
  "customName": false,
  "customRole": false,
  "handler": "admin/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "envVars": [],
    "optimize": {
      "exclude": [ "aws-sdk" ],
      "transforms": [
        {
          "name": "babelify",
          "opts": {
            "presets": [
              "es2015"
            ]
          }
        }
      ]
    }
  },
  "endpoints": [
    {
      "path": "admin",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "auth",
      "customAuthorizer": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{\"query\" : $input.json(\"$\")}"
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  },
  "runtime": "nodejs"
}
