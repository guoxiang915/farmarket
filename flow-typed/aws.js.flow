type LambdaResponse = {
  Payload: string,
};

type Payload = {
  FunctionName: string,
  Payload: string,
};

type Config = {
  apiVersion: string,
  region?: string,
};

declare module 'aws-sdk' {
  declare class Lambda {
    constructor(config: Config): void;
    invoke(params: Payload): {
      promise(): Promise<LambdaResponse>
    }
  }
}
