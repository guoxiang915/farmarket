{
  "name": "serverless-graphql",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql.git"
  },
  "scripts": {
    "test": "jest",
    "migrate": "knex migrate:latest",
    "seed-local": "cd seed-data && node create_seed_data.js && node seed_local.js",
    "seed-prod": "cd seed-data && node create_seed_data.js && node seed_prod.js",
    "seed-prod-batch": "cd seed-data && node create_seed_data.js && node batch_insert.js",
    "start": "yarn migrate && yarn seed-local && env-cmd ../config/security.env.local serverless offline start --httpPort 4000",
    "dev": "env-cmd ../config/security.env.local serverless offline start --httpPort 4000",
    "deploy-prod": "env-cmd ../config/security.env.prod yarn migrate --env production && env-cmd ../config/security.env.prod yarn seed-prod && yarn deploy-sls",
    "deploy-sls": "env-cmd ../config/security.env.prod serverless --stage=dev deploy"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^6.2.7",
    "@graphql-tools/load": "^6.2.8",
    "@graphql-tools/schema": "^7.1.5",
    "apollo-server-lambda": "^2.24.0",
    "apollo-tracing": "^0.14.0",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.12.1",
    "crypto": "^1.0.1",
    "graph": "^0.2.0",
    "graphql": "^15.5.0",
    "graphql-playground-middleware-lambda": "^1.7.23",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.5",
    "mysql": "^2.18.1",
    "objection": "^2.2.15",
    "pg": "^8.6.0",
    "regenerator-runtime": "^0.13.7",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.905.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "env-cmd": "^5.0.0",
    "faker": "^5.5.3",
    "imports-loader": "0.8.0",
    "jest": "^26.6.3",
    "jsonfile": "^6.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^4.1.0",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  }
}
